extends layout

block content
 h1 #{siteTitle}
 div
  input#search(type='text', placeholder='Search for a job' name='keyword')
  button#searchButton(onclick= 'searchJob()') Search
 h3 Jobs found on Posao.hr
 ul
  for val in title
   li
    a(href= val.posao_links)= val.posao_job
 h3 Jobs found on MojPosao.net
 ul
  for val in titleTwo
   li
    a(href= val.mojposao_links)= val.mojposao_job
 h4 Featured jobs on MojPosao.net
 ul
  for val in titleTwo
   li
    a(href= val.featured_links)= val.featured_job

